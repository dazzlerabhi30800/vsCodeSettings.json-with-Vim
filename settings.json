{
    "window.menuBarVisibility": "compact",
    "editor.minimap.enabled": false,
    "editor.lineHeight": 1.4,
    "editor.tokenColorCustomizations": {
        "comments": {
            "foreground": "#5e5e5e",
            "fontStyle": "italic"
        },
        "functions": {
            "fontStyle": "italic"
        },
        "textMateRules": [
            {
                "scope": [
                    "entity.other.attribute-name",
                    "support.constant.property-value.css"
                ],
                "settings": {
                    "fontStyle": "italic"
                }
            }
        ]
    },
    "editor.formatOnSave": true,
    "extensions.experimental.affinity": {
        "vscodevim.vim": 1,
        "asvetliakov.vscode-neovim": 1
    },
    "editor.mouseWheelZoom": true,
    "workbench.colorCustomizations": {
        "editorCursor.foreground": "#eb4087",
        "editorLineNumber.activeForeground": "#f85a87",
        "editor.lineHighlightBackground": "#59575721",
        "editor.background": "#191c1f",
        "editorGutter.background": "#191C1F",
        "activityBar.background": "#191C1F",
        "sideBar.background": "#191C1F",
        "titleBar.activeBackground": "#191C1F",
        "editorGroupHeader.tabsBackground": "#191C1F",
        "tab.activeBackground": "#191C1F",
        "breadcrumb.background": "#191C1F",
        "tab.inactiveBackground": "#191C1F",
        "sideBarSectionHeader.background": "#191C1F",
        "activityBarBadge.background": "#f24a96",
        "terminal.background": "#191C1F",
        "activityBarBadge.foreground": "#141414",
        // "editorSuggestWidget.background": "#0d1921",
        "editorSuggestWidget.background": "#191C1F",
        "activityBar.border": "#29292900"
    },
    "workbench.activityBar.location": "hidden",
    "editor.fontFamily": "FiraCode Nerd Font,BlexMono Nerd Font,Cascadia Code",
    "editor.cursorBlinking": "smooth",
    // "vscode-neovim.neovimExecutablePaths.win32": "C:/Program Files/Neovim/bin/nvim.exe",
    "git.openRepositoryInParentFolders": "never",
    "editor.fontWeight": "400",
    "window.zoomLevel": 1,
    "security.workspace.trust.untrustedFiles": "open",
    // "editor.fontLigatures": "'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'zero', 'onum'",
    "editor.fontLigatures": true,
    "vim.easymotion": true,
    "vim.leader": "<space>",
    "vim.useSystemClipboard": true,
    "window.titleBarStyle": "custom",
    "workbench.statusBar.visible": false,
    "redhat.telemetry.enabled": true,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorWidth": 0.5,
    "editor.cursorWidth": 1,
    "terminal.integrated.enableImages": true,
    "terminal.integrated.gpuAcceleration": "on",
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "glassit.alpha": 255,
    "workbench.colorTheme": "Tailwind - Darkest",
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "workbench.activityBar.visible": false,
    // "editor.fontLigatures": "'calt', 'ss01'",
}
