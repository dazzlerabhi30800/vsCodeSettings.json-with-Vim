
{
    "editor.linkedEditing": true,
    "editor.fontWeight": "300",
    "workbench.list.smoothScrolling": true,
    // "editor.fontFamily": "InconsolataLGC Nerd Font Mono, Consolas, 'Courier New', monospace",
    "editor.fontFamily": "Hack Nerd Font,JetBrainsMono Nerd Font Mono, monospace",
    "editor.mouseWheelZoom": true,
    "editor.cursorWidth": 2,
    "editor.fontLigatures": "'calt', 'ss01'",
    "window.menuBarVisibility": "compact",
    "vim.cursorStylePerMode.insert": "line",
    "codesnap.backgroundColor": "#abb8c300",
    "codesnap.boxShadow": "rgba(0, 0, 0, 0) 0px 20px 68px",
    "terminal.integrated.fontFamily": "CaskaydiaCove Nerd Font",
    "[javascriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "vscode_vibrancy.opacity": 0.88,
    "glassit.alpha": 255,
    "synthwave84.brightness": 0.45,
    "[css]": {
        "editor.defaultFormatter": "vscode.css-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "vim.cursorStylePerMode.normal": "block",
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "terminal.integrated.profiles.windows": {
        "PowerShell": {
            "source": "PowerShell",
            "icon": "terminal-powershell"
        },
        "Command Prompt": {
            "path": [
                "${env:windir}\\Sysnative\\cmd.exe",
                "${env:windir}\\System32\\cmd.exe"
            ],
            "args": [],
            "icon": "terminal-cmd"
        },
        "Git Bash": {
            "source": "Git Bash"
        },
        "Windows PowerShell": {
            "path": "C:\\WINDOWS\\System32\\WindowsPowerShell\\v1.0\\powershell.exe"
        }
    },
    "terminal.integrated.defaultProfile.windows": "Windows PowerShell",
    "editor.formatOnSave": true,
    "editor.smoothScrolling": true,
    "terminal.integrated.smoothScrolling": true,
    "security.workspace.trust.untrustedFiles": "open",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "editor.fontVariations": false,
    "workbench.colorCustomizations": {
        "editor.lineHighlightBackground": "#78787814",
        "editorCursor.foreground": "#f7c35a",
        // "statusBar.background": "#1B1B1B",
        "statusBar.noFolderBackground": "#1B1B1B",
        "statusBar.debuggingBackground": "#1B1B1B",
        "statusBar.foreground": "#afafb0",
        "statusBar.debuggingForeground": "#434C5E"
    },
    "editor.semanticTokenColorCustomizations": {
        "enabled": true,
    },
    "editor.tokenColorCustomizations": {
        "comments": "#838280",
        // "functions": "#1ff7de",
        // "variables": "#d7fa86",
        // "numbers": "#8576f9",
        // "keywords": "#f42f92",
        // "types": "#fb7e8e",
        // "strings": "#daad83",
        "textMateRules": [
            {
                "scope": [
                    "variable.other.readwrite.js.jsx",
                    "entity.name.function.js.jsx",
                    "entity.name.function.js",
                    "entity.name.function.tsx"
                ],
                "settings": {
                    "fontStyle": "italic bold underline",
                    // "fontStyle": "italic"
                }
            },
            {
                "scope": ["keyword.control.normalmode.vim", "constant.normalmode.vim"],
                "settings": {
                    "foreground": "#FF0000",
                },
            },
            {
                "scope": "variable.other.constant.object.js",
                "settings": {
                    // "foreground": "#4f6fc9ee",
                    "fontStyle": "italic",
                }
            },
            {
                "scope": [
                    "variable.other.readwrite.js.jsx"
                ],
                "settings": {
                    "fontStyle": "bold underline"
                }
            },
            {
                "scope": [
                    "comment",
                    "entity.other.attribute-name.js.jsx",
                    "entity.other.attribute-name.class.css",
                    "entity.other.attribute-name.pseudo-class.css",
                    "entity.name.tag.css",
                    "keyword.control.at-rule.media.css",
                    "variable.other.object.js"
                ],
                "settings": {
                    "fontStyle": "italic"
                }
            }
        ]
    },
    "vim.easymotion": true,
    "vim.leader": "<space>",
    "vim.useSystemClipboard": true,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<C-j>"
            ],
            "after": [
                "<C-w>",
                "j"
            ],
        },
        {
            "before": [
                "<C-k>"
            ],
            "after": [
                "<C-w>",
                "k"
            ],
        },
        {
            "before": [
                "<C-h>"
            ],
            "after": [
                "<C-w>",
                "h"
            ],
        },
        {
            "before": [
                "<C-l>"
            ],
            "after": [
                "<C-w>",
                "l"
            ],
        },
        {
            "before": [
                "k"
            ],
            "after": [
                "g",
                "k"
            ],
        },
        {
            "before": [
                "j"
            ],
            "after": [
                "g",
                "j"
            ],
        },
        {
            "before": ["<Leader>","r", "f" ],
            "commands": [
                "fileutils.renameFile"
            ]
        },
        {
            "before": ["K"],
            "commands": [
                "editor.action.showDefinitionPreviewHover"
            ]
        },
        {
            "before": [
                "<Leader>",
                "b"
            ],
            "commands": [
                "workbench.action.toggleSidebarVisibility"
            ],
        },
        
        {
            "before": [
                "<Leader>",
                "r"
            ],
            "commands": [
                "revealInExplorer"
            ],
        },
        {
            "before": [
                "<Leader>",
                "f"
            ],
            "commands": [
                "workbench.action.quickOpen"
            ],
        },
{
    "before": [
        "<Leader>",
        "r"
    ],
            "commands": [
                "revealInExplorer"
            ],
        }, 
        {
            "before": [
